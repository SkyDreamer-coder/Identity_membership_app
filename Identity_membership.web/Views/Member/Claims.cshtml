
@{
    ViewData["Title"] = "Claims";
}
@model List<ClaimVM>
<h1>Claims</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Issuer</th>
            <th>Type</th>
            <th>Value</th>            
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Issuer</td>
                <td>@item.Type</td>
                <td>@item.Value</td>                
            </tr>
        }
    </tbody>
</table>